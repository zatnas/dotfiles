def_key "t"
	find
def_key "T"
	find_item_forward

def_key "+"
	show_clock
def_key "="
	volume_up

def_key "j"
	scroll_down
def_key "k"
	scroll_up

def_key "ctrl-u"
	page_up
def_key "ctrl-d"
	page_down

def_key "h"
	previous_column
def_key "l"
	next_column

def_key "n"
	next_found_item
def_key "N"
	previous_found_item

def_key "J"
	move_sort_order_down
def_key "K"
	move_sort_order_up

def_key "J"
	move_selected_items_down
def_key "K"
	move_selected_items_up

def_key "J"
	select_item
	scroll_down
def_key "K"
	select_item
	scroll_up

def_key "g"
	move_home
def_key "G"
	move_end

def_key "0"
	replay_song
def_key "L"
	seek_forward
def_key "H"
	seek_backward
